{
  "name": "react-elestate",
  "version": "0.0.0",
  "description": "Elevate your React state for access anywhere",
  "main": "dist/index.js",
  "scripts": {
    "dist": "rm -rf dist && tsc --project tsconfig.dist.json",
    "start": "tsb serve",
    "build": "tsb build",
    "prettier": "prettier --write '**/*'",
    "prettier-check": "prettier --check '**/*'",
    "lint": "eslint '**/*.{ts,tsx,js,jsx}' && npm run prettier-check",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakesidsmith/react-elestate.git"
  },
  "keywords": [
    "react",
    "elevate",
    "hoist",
    "state",
    "management",
    "share",
    "access"
  ],
  "author": "Jake 'Sid' Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakesidsmith/react-elestate/issues"
  },
  "homepage": "https://github.com/jakesidsmith/react-elestate#readme",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@jakesidsmith/tsb": "^0.1.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "react": "16.8 - 17",
    "react-dom": "16.8 - 17"
  }
}
